<!-- DataTales Example -->
<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
    <div class="row">
        <% for(let loop = 0; loop < item.imageId.length; loop++) { %>
            <div class="col-md-4">
                <div class="card shadow mb-4 mt-2">
                    <div class="card-body">
                        <img src="https://staycation-dashboard.herokuapp.com/<%= item.imageId[loop].imageUrl %>" alt="image" style="width: 100%; display: block;">
                    </div>
                </div>
            </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-warning">Back</a>
</div>